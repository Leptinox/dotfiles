Xft.dpi: 96
Xft.antialias: true      
Xft.rgba: rgb 
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0050/c800
URxvt.borderColor: rgba:0000/0000/0200/c800

URxvt.scrollBar: false
URxvt.url-launcher: firefox
URxvt.internalBorder: 25

URxvt.font: xft:Fira Code:regular:pixelsize=12:antialias=true:hinting=true,xft:Source Code Pro:medium:pixelsize=12:antialias=true:hinting=true,xft:Noto Mono:pixelsize=12:antialias=true:hinting=true,xft:Symbola
URxvt.boldFont: xft:Fira Code:bold:pixelsize=12:antialias=true:hinting=true,xft:Source Code Pro:semibold:pixelsize=12:antialias=true:hinting=true,xft:Noto Mono:pixelsize=12:antialias=true:hinting=true,xft:Symbola
URxvt.italicFont: xft:Fira Code:italic:pixelsize=12:antialias=true:hinting=true,xft:Source Code Pro:italic:pixelsize=12:antialias=true:hinting=true,xft:Noto Mono:pixelsize=12:antialias=true:hinting=true,xft:Symbola
URxvt.boldItalicFont: xft:Fira Code:bold:italic:pixelsize=12:antialias=true:hinting=true,xft:Source Code Pro:semibold:italic:pixelsize=12:antialias=true:hinting=true,xft:Noto Mono:pixelsize=12:antialias=true:hinting=true,xft:Symbola

URxvt.letterSpace: -1
